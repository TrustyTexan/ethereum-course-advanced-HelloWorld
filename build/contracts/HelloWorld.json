  "source": "pragma solidity 0.5.12;\n\ncontract HelloWorld {\n  string message = \"Hello World!\";\n\n  function getMessage() public view returns (string memory) {\n    return message;\n  }\n\n  function setMessage(string memory newMessage) public payable {\n    require(msg.value >= 1000000 wei, 'Sender must pay at least one ether');\n    message = newMessage;\n  }\n}",
